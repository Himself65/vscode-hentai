{
  "name": "vscode-hentai",
  "displayName": "hentai",
  "description": "code and display hentai image",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/Himself65/vscode-hentai/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Himself65/vscode-hentai.git"
  },
  "homepage": "https://github.com/Himself65/vscode-hentai",
  "publisher": "himself6565",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "configuration": {
    "title": "Hentai Mode",
    "properties": {
      "hentai.enabled": {
        "default": false,
        "type": "boolean",
        "description": "Enable to activate Hentai MODE!!!"
      },
      "hentai.keywords": {
        "default": "saber",
        "type": "string",
        "description": "Keywords which the images will be"
      },
      "hentai.coolingtime": {
        "default": 30,
        "type": "number",
        "description": "Cooling time to switch images (minutes)"
      },
      "hentai.style": {
        "default": {
          "content": "''",
          "pointer-events": "none",
          "position": "absolute",
          "z-index": "99999",
          "width": "100%",
          "height": "100%",
          "background-position": "100% 100%",
          "background-repeat": "no-repeat",
          "opacity": 1
        },
        "type": "object",
        "description": "CSS style on vscode"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c ./build/rollup.config.js",
    "watch": "yarn build --watch",
    "pretest": "yarn build && babel-node ./build/pretest.js",
    "test": "cross-env NODE_ENV=test node ./out/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@types/chai": "^4.2.3",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@types/vscode": "^1.38.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "supertest": "^4.0.2",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3"
  }
}
