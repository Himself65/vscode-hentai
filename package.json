{
  "name": "vscode-hentai",
  "displayName": "vscode-hentai",
  "description": "code and display hentai image",
  "version": "0.1.0",
  "bugs": {
    "url": "https://github.com/Himself65/vscode-hentai/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Himself65/vscode-hentai.git"
  },
  "homepage": "https://github.com/Himself65/vscode-hentai",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "configuration": {
    "title": "Hentai Mode",
    "properties": {
      "hentai.enabled": {
        "default": false,
        "type": "boolean",
        "description": "Enable to activate Hentai MODE!!!"
      },
      "hentai.keywords": {
        "default": "saber",
        "type": "string",
        "description": "Keywords which the images will be"
      },
      "hentai.coolingtime": {
        "default": 30,
        "type": "number",
        "description": "Cooling time to switch images (minutes)"
      },
      "hentai.style": {
        "default": {
          "content": "''",
          "pointer-events": "none",
          "position": "absolute",
          "z-index": "99999",
          "width": "100%",
          "height": "100%",
          "background-position": "100% 100%",
          "background-repeat": "no-repeat",
          "opacity": 1
        },
        "type": "object",
        "description": "CSS style on vscode"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c ./build/rollup.config.js",
    "pretest": "babel-node ./build/pretest.js",
    "test": "cross-env NODE_ENV=test node ./out/index.js"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/supertest": "^2.0.8",
    "@types/vscode": "^1.36.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "jest-serializer-html": "^7.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  }
}
